{
  "name": "@kui-shell/plugin-search",
  "version": "0.0.0-semantically-released",
  "description": "KUI plugin for MCM/ICP Search.",
  "author": "IBM Multicloud Manager/Cloud Private",
  "license": "ISC",
  "keywords": [
    "kui",
    "plugin",
    "search",
    "icp",
    "mcm"
  ],
  "main": "dist/plugin.js",
  "module": "mdist/plugin.js",
  "types": "mdist/plugin.d.ts",
  "private": true,
  "scripts": {
    "test:chrome": "nightwatch ./build-tools/tests/e2e -e chrome",
    "test:firefox": "nightwatch ./build-tools/tests/e2e -e firefox",
    "test:safari": "nightwatch ./build-tools/tests/e2e -e safari",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "scss": "sass --watch src/src-web/styles/index.scss src/src-web/styles/index.css",
    "buildCSS": "sass src/src-web/styles/index.scss src/src-web/styles/index.css"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "carbon-components": "^10.9.0",
    "carbon-components-react": "^7.9.0",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-tag-autocomplete": "^5.11.2",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@kui-shell/core": "^5.7.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "chalk": "^2.4.2",
    "chromedriver": "^78.0.1",
    "commitizen": "^3.1.2",
    "del": "^5.1.0",
    "electron": "^7.1.8",
    "geckodriver": "^1.19.1",
    "ghooks": "^2.0.4",
    "nightwatch": "^1.2.1",
    "sass": "^1.24.2",
    "semantic-release": "^15.14.0",
    "xml2json": "^0.11.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/IBMPrivateCloud/search-kui-plugin"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "./plugin-search.tgz",
              "label": "KUI plugin-search package"
            }
          ]
        }
      ]
    ]
  }
}
